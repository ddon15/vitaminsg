background-color: #fff !important;
color: #f7941d !important;
}
#header #cart .checkout .mini-cart-button {
background-color: #f7941d;
color: #fff;
border: 1px solid #fff;
}
#header #cart .checkout .mini-cart-button:hover {
background-color: #fff !important;
color: #f7941d !important;
}
#welcome a,
