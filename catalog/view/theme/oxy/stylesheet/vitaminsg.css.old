padding-top: 15px;
}
.success, .warning, .attention, .information {
padding: 20px 40px 20px 110px;
min-height: 80px;
}
.success, .warning, .attention, .information,
#header #cart .content {
background-color: #f7941d !important;
color: #fff;
border-radius: 3px;
}
.success a, .warning a, .attention a, .information a,
.success a:visited, .warning a:visited, .attention a:visited, .information a:visited,
.mini-cart-info td a, .mini-cart-total td a,
.mini-cart-info .name small,
.mini-cart-info td a:visited, .mini-cart-total td a:visited {
color: #fff !important;
font-weight: bold;
}
.success a:active, .warning a:active, .attention a:active, .information a:active,
.success a:hover, .warning a:hover, .attention a:hover, .information a:hover,
.mini-cart-info td a:active, .mini-cart-total td a:active,
.mini-cart-info td a:hover, .mini-cart-total td a:hover {
text-decoration: underline;
}
.mini-cart-info td, .mini-cart-total {
border-bottom: 1px dotted #fff !important;
}
.mini-cart-info td, .mini-cart-total td {
color: #fff !important;
}
#header #cart .checkout .mini-checkout-button {
background-color: #f7941d;
color: #fff;
border: 1px solid #fff;
}
