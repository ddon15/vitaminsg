@media only screen and (max-width: 1009px)
{
	div.premium-member-perks {
	height: 236px;
	}
	div.premium-member-perk-2 {
	font-size: 14px;
	}
	.product-box-slider-flexslider .slides > li:first-child {
	max-width: 30%;
	}
	#footer_c_4 {
	position: relative;
	right: inherit;
	float: right;
	width: 30% !important;
	}
}
@media (max-width: 979px) and (min-width: 768px)
{
	#t-header {
	min-height: 160px;
	}
	.ei-title {
	margin-right: 8%;
	margin-left: 6%;
	}
	.product-details-member-price, .product-details-sale-price {
	padding-left: 0;
	font-size: 32px;
	}
	.product-details-price-original-price {
	font-size: 44px;
	}
	.product-buy h1 {
	font-size: 20px;
	}
	.product-details-price-block {
	width: 45%;
	}
	.product-grid .name {
	height: 70px;
	}
}
@media only screen and (max-width: 767px)
{
	#vit_header_account,
	#vit_header_phone {
	text-align: center;
	margin: 20px 0;
	}
	#header #search {
	margin-top: 22px !important;
	margin-left: 0;
	}
	#header #cart {
	top: 18px;
	}
	.sec-menu-right li {
	float: left;
	}
	#footer_c_4 {
	width: 50% !important;
	}
	div.premium-member-perks {
	width: 50%;
	height: 320px;
	}
	div.premium-member-perk-2 {
	font-size: 16px;
	}
	.premium-member-perk-1 img.premium-member-perks-img {
	width: 65%;
	}
	div.premium-member-perks-title-left {
	float: none;
	margin-bottom: 20px;
	}
	div.premium-member-perks-title-right {
	float: none;
	}
	.product-box-slider-flexslider .slides > li:first-child {
	max-width: 50%;
	}
	#footer_p {
	border-top: 1px solid #e6e6e6;
	}
	#footer_p span.p_title {
	padding: 10px 0;
	}
	#footer_p .p_content {
	margin-bottom: 30px;
	}
	.oxy-banner div {
	margin-bottom: 0;
	}
	.ei-slider-large li img {
	padding: 10px;
	margin-top: 0px !important;
	position: relative;
	width: 50% !important;
	margin-left: 25%!important;
	margin-right: 25%!important;
	}
	.ei-title {
	position: relative;
	}
	.ei-slider {
	height: 500px;
	}
	.ei-title h4 a.button {
	min-width: 150px;
	}
	.product-right-sm {
	border: 0;
	}
	.product-details-price-block {
	width: 35%;
	width: 35%;
	text-align: center;
	}
	span.sale-icon {
	left: 10px;
	}
}
@media screen and (max-width:479px)
{
	#footer_c_4 {
	width: 70% !important;
	}
	div.premium-member-perks {
	max-height: 236px;
	}
	.manufacturer-list .manufacturer-heading a {
