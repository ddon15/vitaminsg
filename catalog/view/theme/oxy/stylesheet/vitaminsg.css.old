/*------- [SB]------------ */
.clearfix:after {
font-size: 0px;
content: ".";
display: block;
height: 0px;
visibility: hidden;
clear: both;
}
.left {
float: left;
}
.right {
float: right;
}
span.red-text {
color: #ef4035;
}
span.green-text {
color: #00a651;
}
span.input_instructions {
